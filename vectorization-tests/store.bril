@main {
    size: int = const 4;
    data: int = const 1;
    offset: int = const 1;
    one: int = const 1;
    arr: ptr<int> = alloc size;
    cpyarr: ptr<int> = id arr;

    i: int = const 0;
.header:
    cond: bool = eq i size;
    br cond .end .loop;
.loop:
    two: int = const 2;
    res: int = add data data;
    final: int = mul res two;

    store cpyarr final;
    cpyarr: ptr<int> = ptradd cpyarr offset;
    i: int = add one i;
    jmp .header;
.end:

    cpyarr: ptr<int> = id arr;
    i: int = const 0;
.header1:
    cond: bool = eq i size;
    br cond .end1 .loop1;
.loop1:
    res: int = load cpyarr;
    print res

    cpyarr: ptr<int> = ptradd cpyarr offset;
    i: int = add one i;
    jmp .header1;
.end1:

  
    free arr;
}